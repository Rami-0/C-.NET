@model IEnumerable<DAL.Entities.Project>

<h2>Projects</h2>

<table class="table">
    <thead>
        <tr>
            <th>Project Name</th>
            <th>Start Date</th>
            <th>End Date</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var project in Model)
        {
            <tr>
                <td>@project.ProjectName</td>
                <td>@project.StartDate.ToShortDateString()</td>
                <td>@project.EndDate.ToShortDateString()</td>
                <td>
                    <a href="@Url.Action("Edit", new { id = project.Id })">Edit</a> |
                    <a href="@Url.Action("Delete", new { id = project.Id })">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>

